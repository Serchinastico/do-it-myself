{
  "scripts": {
    "build:android": "eas build --platform android --profile production",
    "build:android:local": "eas build --platform android --local",
    "build:ios": "eas build --platform ios --profile production",
    "build:ios:local": "eas build --platform ios --local",
    "submit:android": "eas submit --platform android --latest",
    "submit:ios": "eas submit --platform ios --latest",
    "editor:build": "pnpm run --workspace-concurrency=1 '/^editor:build:/'",
    "editor:build:manual:light": "TOOL=manual COLOR_SCHEME=light vite --config ./src/editor-web/vite.config.ts build && node ./node_modules/@10play/tentap-editor/scripts/buildEditor.js ./src/editor-web/build-manual-light/index.html",
    "editor:build:manual:dark": "TOOL=manual COLOR_SCHEME=dark vite --config ./src/editor-web/vite.config.ts build && node ./node_modules/@10play/tentap-editor/scripts/buildEditor.js ./src/editor-web/build-manual-dark/index.html",
    "editor:build:worklog:light": "TOOL=worklog COLOR_SCHEME=light vite --config ./src/editor-web/vite.config.ts build && node ./node_modules/@10play/tentap-editor/scripts/buildEditor.js ./src/editor-web/build-worklog-light/index.html",
    "editor:build:worklog:dark": "TOOL=worklog COLOR_SCHEME=dark vite --config ./src/editor-web/vite.config.ts build && node ./node_modules/@10play/tentap-editor/scripts/buildEditor.js ./src/editor-web/build-worklog-dark/index.html",
    "i18n:compile": "lingui compile --typescript",
    "i18n:extract": "lingui extract",
    "lint": "run-s lint:expo lint:eslint",
    "lint:eslint": "pnpm run --filter eslint-plugin-dim build && eslint \"**/*.{ts,tsx}\"",
    "lint:eslint:fix": "pnpm run --filter eslint-plugin-dim build && eslint \"**/*.{ts,tsx}\" --fix",
    "lint:expo": "expo-doctor",
    "prebuild": "APP_VARIANT=development expo prebuild --clean",
    "preinstall": "npx only-allow pnpm",
    "prepare": "if [ \"$LEFTHOOK\" != \"false\" ]; then lefthook install; fi",
    "start": "APP_VARIANT=development expo start -d",
    "android": "APP_VARIANT=development expo run:android -d",
    "ios": "APP_VARIANT=development expo run:ios -d"
  },
  "main": "./src/index.ts",
  "name": "do-it-myself",
  "version": "1.0.0",
  "dependencies": {
    "@10play/tentap-editor": "^0.5.24",
    "@likashefqet/react-native-image-zoom": "^4.2.1",
