    "dayjs": "^1.11.13",
    "expo": "^51.0.39",
    "expo-asset": "~10.0.10",
    "expo-av": "~14.0.7",
    "expo-constants": "~16.0.2",
    "expo-file-system": "^17.0.1",
    "expo-image": "~1.13.0",
